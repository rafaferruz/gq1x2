
<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
	      xmlns="http://java.sun.com/xml/ns/javaee"
	      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <application>
	<locale-config>
	    <default-locale>es</default-locale>
	    <supported-locale>en</supported-locale>
	</locale-config>
	<resource-bundle>
	    <base-name>com.gq2.multilang.messages</base-name>
	    <var>msg</var>
	</resource-bundle>
	<message-bundle>com.gq2.multilang.errors</message-bundle>
    </application>

    <navigation-rule>
	<from-view-id>/*</from-view-id>
	<navigation-case>
	    <from-outcome>championshipList</from-outcome>
	    <to-view-id>/championshipList.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>teamsList</from-outcome>
	    <to-view-id>/teamsList.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>showScores</from-outcome>
	    <to-view-id>/showScores.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>createMatches</from-outcome>
	    <to-view-id>/createMatches.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>enrolledTeams</from-outcome>
	    <to-view-id>/enrolledTeams.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>showClassification</from-outcome>
	    <to-view-id>/showClassification.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>generateClassification</from-outcome>
	    <to-view-id>/generateClassification.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>generateSuperTable</from-outcome>
	    <to-view-id>/generateSuperTable.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>generatePrognostic</from-outcome>
	    <to-view-id>/generatePrognostic.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>generatePrePool</from-outcome>
	    <to-view-id>/generatePrePool.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>prePool</from-outcome>
	    <to-view-id>/prePool.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>statPrognostics</from-outcome>
	    <to-view-id>/statPrognostics.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>bets</from-outcome>
	    <to-view-id>/bets.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>columns</from-outcome>
	    <to-view-id>/columns.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>Home</from-outcome>
	    <to-view-id>/index.xhtml</to-view-id>
	</navigation-case>
    </navigation-rule>
    <navigation-rule>
	<from-view-id>/columns.xhtml</from-view-id>
	<navigation-case>
	    <from-outcome>printBetForms</from-outcome>
	    <to-view-id>/printBets.xhtml</to-view-id>
	</navigation-case>
    </navigation-rule>

    <navigation-rule>
	<from-view-id>/championshipList.xhtml</from-view-id>
	<navigation-case>
	    <from-outcome>editChampionship</from-outcome>
	    <to-view-id>/championship.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>newChampionship</from-outcome>
	    <to-view-id>/championship.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>deleteChampionship</from-outcome>
	    <to-view-id>/championshipList.xhtml</to-view-id>
	</navigation-case>
    </navigation-rule>
    
    <navigation-rule>
	<from-view-id>/championship.xhtml</from-view-id>
	<navigation-case>
	    <from-outcome>editChampionship</from-outcome>
	    <to-view-id>/championshipList.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>newChampionship</from-outcome>
	    <to-view-id>/championshipList.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>cancelChampionship</from-outcome>
	    <to-view-id>/championshipList.xhtml</to-view-id>
	</navigation-case>
    </navigation-rule>
    
    <navigation-rule>
	<from-view-id>/teamList.xhtml</from-view-id>
	<navigation-case>
	    <from-outcome>editTeam</from-outcome>
	    <to-view-id>/team.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>newTeam</from-outcome>
	    <to-view-id>/team.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>deleteTeam</from-outcome>
	    <to-view-id>/teamList.xhtml</to-view-id>
	</navigation-case>
    </navigation-rule>
    
    <navigation-rule>
	<from-view-id>/team.xhtml</from-view-id>
	<navigation-case>
	    <from-outcome>editTeam</from-outcome>
	    <to-view-id>/teamList.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>newTeam</from-outcome>
	    <to-view-id>/teamList.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	    <from-outcome>cancelTeam</from-outcome>
	    <to-view-id>/teamList.xhtml</to-view-id>
	</navigation-case>
    </navigation-rule>
    
</faces-config>
